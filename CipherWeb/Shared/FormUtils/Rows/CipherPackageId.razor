@inherits CipherAutoComplete

@code {
    /// <summary>
    /// Value of the component.
    /// </summary>
    [Parameter]
    public string? PackId { get; set; }

    protected override async Task OnInitializedAsync()
    {
        Label ??= "מספר תעודה";
        Icon ??= "tag";
    }
}

<CipherAutoComplete Width="@Width" Label="@Label" Icon="@Icon" Style="@Constants.textStyle" @bind-Value=@PackId/>