@using Microsoft.AspNetCore.Components.Forms

@inherits CipherFormRow

<CipherAutoComplete @ref=cipherAutoComplete Icon="@Icons.Person" Label="@LabelText" Data="@TestedData.Workers"  />


@code {
    [Parameter]
    public string? Worker { get; set; } = Constants.SetUser.FullName();

    [Parameter]
    public string? LabelText { get; set; }

    [Parameter]
    public bool Disabled { get; set; }

    CipherAutoComplete cipherAutoComplete = new CipherAutoComplete();

    protected override async Task OnInitializedAsync()
    {
        LabelText = "ממלא הטופס";
    }
}