@inherits CipherResourceTable<ICategory>

<CipherResourceTable Cipher_TItem="ICategory" ChosenObjects="@ChosenObjects"
                     PrefixFileTitle="categories" Title="@Title" WithHeaderContent="false" />

@code {
    protected override async Task OnInitializedAsync()
    {
        Title ??= Translator.GetTranslation("Categories");

        var result = await CachedData.AllCategories;
        ChosenObjects = result.Item1;
    }
}