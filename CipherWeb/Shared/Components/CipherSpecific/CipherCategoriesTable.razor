@inherits CipherResourceTable<ICategory>

<CipherResourceTable Cipher_TItem="ICategory" ChosenObjects="@ChosenObjects" SelectedColumns="@SelectedColumns"
                     PrefixFileTitle="categories" Title="@Title" WithHeaderContent="false" />

@code {
    protected override void OnInitialized()
    {
        Title ??= Translator.GetTranslation("Categories");

        SelectedColumns = new List<string>() {
            nameof(ICategory.Id),
            nameof(ICategory.Name),
            nameof(ICategory.Description),
            nameof(ICategory.MaterialType),
        };
    }
}