@inherits CipherResourceTable<IUnit>

<CipherResourceTable Cipher_TItem="IUnit" ChosenObjects="@ChosenObjects"
                     PrefixFileTitle="units" Title="@Title" WithHeaderContent="false" />

@code {
    protected override async Task OnInitializedAsync()
    {
        Title ??= Translator.GetTranslation("Units");

        var result = await CachedData.AllUnits;
        ChosenObjects = result.Item1;
    }
}