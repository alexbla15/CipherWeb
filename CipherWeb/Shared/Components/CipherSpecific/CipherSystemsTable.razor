@inherits CipherResourceTable<IStorageSystem>

<CipherResourceTable Cipher_TItem="IStorageSystem" ChosenObjects="@ChosenObjects" SelectedColumns="@SelectedColumns"
                     PrefixFileTitle="systems" Title="@Title" WithHeaderContent="false" />

@code {
    protected override void OnInitialized()
    {
        Title ??= Translator.TranslateText("Systems");

        SelectedColumns = new List<string>() {
            nameof(IStorageSystem.Id),
            nameof(IStorageSystem.Name),
            nameof(IStorageSystem.Unit),
            nameof(IStorageSystem.Description),
        };
    }
}