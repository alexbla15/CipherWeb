@inherits CipherResourceTable<IStorageSystem>

<CipherResourceTable Cipher_TItem="IStorageSystem" ChosenObjects="@ChosenObjects"
                     PrefixFileTitle="systems" Title="@Title" WithHeaderContent="false" />

@code {
    protected override async Task OnInitializedAsync()
    {
        Title ??= Translator.GetTranslation("Systems");

        var result = await CachedData.AllSystems;
        ChosenObjects = result.Item1;
    }
}