@inherits RadzenImage

@code {
    /// <summary>
    /// Path to the image.
    /// </summary>
    [Parameter]
    public string LogoPath { get; set; } = "Images/CIPHER_Logo_4.png";

    [Parameter]
    public string Class { get; set; } = Constants.Styles.Pics.Logo;
}

<RadzenImage Path="@LogoPath" class="@Class" />